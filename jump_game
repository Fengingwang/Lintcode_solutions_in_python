class Solution:
    """
    @param A: A list of integers
    @return: A boolean
    """
    def canJump(self, A):
        # write your code here
        
        if len(A) == 0:
            return 
        
        # intial 
        n = len(A)
        dp = [False for i in range(n)]
        dp[0] = True
        
        # status and transfer
        for i in range(1, n):
            for j in range(0, i):
                if dp[j] == True and j + A[j] >= i:
                    dp[i] = True
                    break
        
        # return 
        return dp[n - 1]
            
