
# dp is time limited exceeded!
class Solution:
    """
    @param A: A list of integers
    @return: An integer
    """
    def jump(self, A):
        # write your code here
        if len(A) == 0:
            return 
        
        # intial 
        n = len(A)
        dp = [sys.maxsize for x in range(n)]
        dp[0] = 0
        
        # status and transfer
        for i in range(1, n):
            for j in range(0, i):
                if dp[j] != sys.maxsize and j + A[j] >= i:
                    dp[i] = min(dp[j] + 1, dp[i])
        
        # return 
        return dp[n - 1]
