# DFS template for binary tree

# traverse
def dfs_traverse(self, root):
    if root is None:
        return 
    
    self.dfs_traverse(root.left)
    self.dfs_traverse(root.right)
    
# divide & conquer
def dfs_dc(self, root):
    if root is None:
        return 
    
    # divide 
    left = dfs_dc(root.left)
    right = dfs_dc(root.right)
    
    # conquer
    res = merge from left to right 
    return res 
    
    
