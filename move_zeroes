class Solution:
    """
    @param nums: an integer array
    @return: nothing
    """
    def moveZeroes(self, nums):
        # write your code here
        if nums is None or len(nums) == 0:
            return 
        
        n = len(nums)
        tail = 0
        
        for i in range(n):
            if nums[i] != 0:
                nums[tail], nums[i] = nums[i], nums[tail]
                tail += 1
